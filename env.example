# Frontend Environment Variables
# Copy this file to .env and update the values

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_FRONTEND_URL=http://localhost:3000

# App Configuration
VITE_APP_NAME=Sajha Bajha
VITE_APP_DESCRIPTION=Premium Musical Instruments Store
VITE_APP_VERSION=1.0.0

# Cloudinary Configuration (for image uploads)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Google Analytics (optional)
VITE_GA_TRACKING_ID=your_ga_tracking_id

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=false
VITE_ENABLE_OFFLINE_MODE=false

# Development Configuration
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=false

# Payment Configuration (if using client-side payment processing)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/sajhabajha
VITE_INSTAGRAM_URL=https://instagram.com/sajhabajha
VITE_TWITTER_URL=https://twitter.com/sajhabajha
VITE_YOUTUBE_URL=https://youtube.com/sajhabajha

# Contact Information
VITE_CONTACT_EMAIL=info@sajhabajha.com
VITE_CONTACT_PHONE=+977-1-4XXXXXX
VITE_CONTACT_ADDRESS=Kathmandu, Nepal

# Business Information
VITE_BUSINESS_NAME=Sajha Bajha
VITE_BUSINESS_DESCRIPTION=Your trusted source for premium musical instruments
VITE_BUSINESS_WEBSITE=https://sajhabajha.com 